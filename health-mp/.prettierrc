{
    "useTabs": false,
    "tabWidth": 2,
    "printWidth": 120,
    "singleQuote": true,
    "quoteProps": "as-needed",
    "jsxSingleQuote": true,
    "trailingComma": "all",
    "bracketSpacing": true,
    "jsxBracketSameLine": true,
    "arrowParens": "always",
    "requirePragma": false,
    "insertPragma":false,
    "proseWrap":"preserve",
    "vueIndentScriptAndStyle": false,
    "endOfLine":"lf",
    "semi": true,
    "htmlWhitespaceSensitivity": "ignore",
    "embeddedLanguageFormatting":"auto"
}
